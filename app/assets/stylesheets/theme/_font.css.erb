@font-face {
    font-family: 'johnsans_text_proregular';
    src: url('<%= font_path("johnsanstext-webfont.eot") %>');
    src: url('<%= font_path("johnsanstext-webfont.eot") %>?#iefix') format('embedded-opentype'), url('<%= font_path("johnsanstext-webfont.woff") %>') format('woff'), url('<%= font_path("johnsanstext-webfont.ttf") %>') format('truetype'), url('<%= font_path("johnsanstext-webfont.svg") %>#johnsans_text_proregular') format('svg');
    font-weight: normal;
    font-style: normal;

}
@font-face {
    font-family: 'johnsans_text_probold';
    src: url('<%= font_path("johnsanstextbold-webfont.eot") %>');
    src: url('<%= font_path("johnsanstextbold-webfont.eot") %>?#iefix') format('embedded-opentype'), url('<%= font_path("johnsanstextbold-webfont.woff") %>') format('woff'), url('<%= font_path("johnsanstextbold-webfont.ttf") %>') format('truetype'), url('<%= font_path("johnsanstextbold-webfont.svg") %>#johnsans_text_probold') format('svg');

    font-weight: normal;
    font-style: normal;                 
                                               
}