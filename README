cd ~/Github/gko_letoiny_com
tail -f log/development.log

cd ~/Github/gko_letoiny_com
tail -f log/production.log

cd ~/Github/gko_letoiny_com
bundle install
rails s

cd ~/Github/gko_letoiny_com
bundle exec rake assets:precompile

